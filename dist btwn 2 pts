#include <stdio.h>
#include<math.h>
int main(){
    float x1, y1, x2, y2, dist;
    printf("enter coordinates of x1\n");
    scanf("%f %f", &x1, &x2);
     
    printf("enter coordinates of y1\n");
    scanf("%f %f", &y1, &y2);
    
    dist=sqrt(x2-x1)*(x2-x1)+(y2-y1)*(y2-y1);
    printf("dist btwn two pts %.2f", dist);
     
    
}
