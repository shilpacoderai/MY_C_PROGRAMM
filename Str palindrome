#include <stdio.h>
#include <string.h>

int main() {
char str[30], rev[30];
int i, len;
printf("enter string:");
scanf("%s", &str );
len=strlen(str);

for(i=0;i<len;i++){
    rev[i]=str[len-1-i];
}
rev[len]=rev[len]/10;
if(strcmp(str,rev)==0){
    printf("it is a palindrome ");
}
else
{
    printf("not a palindrome ");
}
    

}
