#include<stdio.h>
int main()
{
    int row,col;
    printf("Enter the rows and columns\n");
    scanf("%d%d",&row,&col);
    int a[row][col], b[row][col],sum[row][col];
    printf("Enter elements of matrix A:\n");
    for (int i = 0; i<row; i++) {
    for (int j = 0; j<col; j++) {
    scanf("%d", &a[i][j]);
  }
 }
 printf("Enter elements of matrix B:\n");
    for (int i = 0; i<row; i++) {
    for (int j = 0; j<col; j++) {
    scanf("%d", &b[i][j]);
  }
 }
 for (int i = 0; i<row; i++) {
for (int j = 0; j<col; j++) {
sum[i][j] = a[i][j] + b[i][j];
}
}
   printf("sum of two matrix:\n");
   for (int i = 0; i<row; i++) {
   for (int j = 0; j<col; j++) {
   printf("%d ", sum[i][j]);
   }
   }
}
